{
  "manifest_version": 3,
  "name": "Deodorant",
  "version": "1.0",
  "description": "Replaces occurrences of 'Elon Musk' or 'Musk' with a user-defined word.",
  "permissions": ["storage"],
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
